    },
    "authors": [
        {
            "name": "Ollie Treend",
            "email": "ollie.treend@digital.justice.gov.uk"
        },
        {
            "name": "Damien Wilson",
            "email": "damien.wilson@digital.justice.gov.uk"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
